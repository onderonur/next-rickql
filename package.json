    "immer": "^7.0.5",
    "next": "^9.4.4",
    "next-seo": "^4.7.2",
    "next-with-apollo": "^5.1.0",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
