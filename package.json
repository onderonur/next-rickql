{
  "name": "rick-and-morty-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "gql-gen": "graphql-codegen --config codegen.yml",
